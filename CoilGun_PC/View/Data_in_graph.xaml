<UserControl x:Class="CoilGun_PC.View.Data_in_graph"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CoilGun_PC.View"
             mc:Ignorable="d" 
             d:DesignHeight="115" d:DesignWidth="200">
    <Grid>
        <TextBlock Text="Název dat:" TextAlignment="Left" Style="{StaticResource SecondText}" Margin="10,10,105,85"/>
        <TextBox x:Name="Data_name_tb"  Style="{StaticResource SecondTextBox}" Margin="100,10,10,85"/>

        <TextBlock Text="Data na osách" TextAlignment="Left" Style="{StaticResource SecondText}" Margin="10,35,10,60"/>

        <TextBlock Text="Osa x:" TextAlignment="Left" Style="{StaticResource SecondText}" Margin="10,60,119,35"/>
        <ComboBox Name="CB_x" SelectionChanged="CB_x_SelectionChanged" Style="{StaticResource ComboBoxStyle}" Margin="86,60,10,35"/>

        <TextBlock Text="Osa y:" TextAlignment="Left" Style="{StaticResource SecondText}" Margin="10,82,119,13"/>
        <ComboBox Name="CB_y" SelectionChanged="CB_y_SelectionChanged" Style="{StaticResource ComboBoxStyle}" Margin="86,85,10,10"/>
    </Grid>
</UserControl>
